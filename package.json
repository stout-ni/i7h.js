{
  "name": "i7h.js",
  "version": "1.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:cjs build:esm build:type",
    "build:cjs": "tsc --target ES2015 --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --target ESNext --module ESNext --outDir dist/esm",
    "build:type": "tsc --declaration --emitDeclarationOnly --outDir dist/types"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.3"
  }
}
